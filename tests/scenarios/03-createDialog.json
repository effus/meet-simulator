{
    "locators": "locators-mobile.json",
    "screenshots": {
        "path": "/screenshots/",
        "prefix": "03-"
    },
    "ignoreConsoleMessages": {
        "types": [
            "log", "debug", "info", "trace"
        ],
        "mask": [
            "verbose @ [DOM] Input elements should have autocomplete attributes"
        ]
    },
    "scenarios": [
        {
            "method": "resetChats",
            "params": {
                "url": "http://localhost:5000/"
            }
        },
        {
            "method": "resetDialogs",
            "params": {
                "url": "http://localhost:5000/"
            }
        },
        {
            "method": "openLogin",
            "params": {
                "url": "http://localhost:8082/"
            }
        },
        {
            "method": "submitLogin",
            "params": {
                "email": "root@test.ee",
                "password": "123123123"
            }
        },
        {
            "method": "selectMenuBotEditor"
        },
        {
            "method": "editBotStory",
            "params": {
                "botIndex": 1
            }
        },
        {
            "method": "addNewMessagesToStory",
            "params": {
                "isFirst": true,
                "firstUserAnswer": "Hi!",
                "secondUserAnswer": "Shit!",
                "botSecondMessage": "Hey!",
                "botThirdMessage": "Yea.. Its funny"
            }
        }
    ]
}